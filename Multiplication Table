import java.util.Scanner;
public class MyInfo {
    public static void main(String[] args){
        System.out.println("Enter a Number");

        Scanner input = new Scanner(System.in);

        int num, sum = 1;

        num = input.nextInt();

        for ( int i = 1; i <= 10 ; i++ )
        {
            System.out.println(num + " x " + i + " = " + (num*i));
        }

        //System.out.println(sum);
        
    }
}
