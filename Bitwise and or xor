import java.util.Scanner;
public class MyInfo {
    public static void main(String[] args){
        System.out.println("Enter Two Number");

        Scanner input = new Scanner(System.in);

        int num, num1;

        num = input.nextInt();
        num1 = input.nextInt();

        System.out.println(num | num1);
        System.out.println(num & num1);
        System.out.println(num ^ num1);
        
    }
}
