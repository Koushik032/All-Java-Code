import java.util.Scanner;
public class MyInfo {
    public static void main(String[] args){
        System.out.println("Enter a Number");

        Scanner input = new Scanner(System.in);

        int num, sum = 1;

        num = input.nextInt();

        for ( int i = 1; i <= num ; i++ )
        {
            sum *= i;
        }

        System.out.println(sum);
        
    }
}
