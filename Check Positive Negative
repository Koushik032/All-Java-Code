import java.util.Scanner;
public class MyInfo {
    public static void main(String[] args){
        System.out.println("Enter any Number");

        Scanner input = new Scanner(System.in);

        int num;

        num = input.nextInt();

        if ( num > 0 )
            System.out.println("Positive Number");
        else
        {
            System.out.println("Negative Number");
        }
    }
}
