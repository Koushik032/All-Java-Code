import java.util.Scanner;
public class MyInfo {
    public static void main(String[] args){
        System.out.println("Enter any Number");

        Scanner input = new Scanner(System.in);

        double Fer;

        Fer = input.nextDouble();

        double Cel = (1.8) * (Fer - 32);

        System.out.println(Cel);
    }
}
